    "react": "https://esm.sh/v135/react@18.3.1",
    "react/": "https://esm.sh/v135/react@18.3.1/"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "export TASK=prod && deno task lume",
    "serve": "export TASK=dev && deno task lume -s",
    "cms": "export TASK=dev && deno task lume cms",
    "esm:add": "deno run -A https://esm.sh/v135 add",
    "esm:update": "deno run -A https://esm.sh/v135 update",
    "esm:remove": "deno run -A https://esm.sh/v135 remove"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/v135/react@18.3.1",
    "jsxImportSourceTypes": "https://esm.sh/v135/@types/react@18.3.1",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ]
  },
  "scopes": {
    "https://esm.sh/v135/": {
      "@bufbuild/protobuf": "https://esm.sh/v135/@bufbuild/protobuf@2.2.3",
      "@radix-ui/primitive": "https://esm.sh/v135/@radix-ui/primitive@1.1.1",
