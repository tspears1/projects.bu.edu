{
  "imports": {
    "@components/": "./app/js/components/",
    "@constants/": "./app/js/constants/",
    "@context/": "./app/js/context/",
    "@hooks/": "./app/js/hooks/",
    "@types/react": "https://esm.sh/@types/react@18.3.1",
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@8091117168e698cc6d83a98b4e87d9e0bcb62852/",
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@8be368cc66c865cc3f4bd47d1251102edf65b736/",
    "@radix-ui/react-dialog": "https://esm.sh/@radix-ui/react-dialog@1.1.4?deps=react@18.3.1",
    "@radix-ui/react-dropdown-menu": "https://esm.sh/@radix-ui/react-dropdown-menu@2.1.4?deps=react@18.3.1",
    "@radix-ui/react-separator": "https://esm.sh/@radix-ui/react-separator@1.1.1?deps=react@18.3.1",
    "@radix-ui/react-slot": "https://esm.sh/@radix-ui/react-slot@1.1.1?deps=react@18.3.1",
    "@radix-ui/react-tooltip": "https://esm.sh/@radix-ui/react-tooltip@1.1.6?deps=react@18.3.1",
    "classnames": "https://esm.sh/classnames@2.5.1",
    "react": "https://esm.sh/react@18.3.1",
    "react-dom": "https://esm.sh/react-dom@18.3.1",
    "react-dom/": "https://esm.sh/react-dom@18.3.1/",
    "react-inlinesvg": "https://esm.sh/react-inlinesvg@4.1.5"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "export TASK=prod && deno task lume",
    "serve": "export TASK=dev && deno task lume -s",
    "cms": "deno task lume cms",
    "esm:add": "deno run -A https://esm.sh/v135 add",
    "esm:update": "deno run -A https://esm.sh/v135 update",
    "esm:remove": "deno run -A https://esm.sh/v135 remove"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react@18.3.1",
    "jsxImportSourceTypes": "npm:@types/react@18.3.1",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ]
  }
}
