{
  "imports": {
    "@components/": "./src/_includes/js/components/",
    "@context/": "./src/_includes/js/context/",
    "@hooks/": "./src/_includes/js/hooks/",
    "@phosphor-icons/react": "npm:@phosphor-icons/react@^2.1.7",
    "classnames": "npm:classnames@^2.5.1",
    "lume/": "https://deno.land/x/lume@v2.4.3/",
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@v0.7.7/",
    "react": "https://esm.sh/react",
    "react-dom/": "https://esm.sh/react-dom/",
    "valtio": "npm:valtio@2.1.2"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "export TASK=prod && deno task lume",
    "serve": "export TASK=dev && deno task lume -s",
    "cms": "export TASK=dev && deno task lume cms"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react",
    "jsxImportSourceTypes": "npm:@types/react",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ]
  }
}
