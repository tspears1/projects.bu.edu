{
  "imports": {
    "@components/": "./app/js/components/",
    "@constants/": "./app/js/constants/",
    "@context/": "./app/js/context/",
    "@hooks/": "./app/js/hooks/",
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@v0.7.7/",
    "lume/": "https://deno.land/x/lume@v2.4.3/",
    "@phosphor-icons/react": "https://esm.sh/v135/*@phosphor-icons/react@2.1.7",
    "@phosphor-icons/react/": "https://esm.sh/v135/*@phosphor-icons/react@2.1.7/",
    "@radix-ui/": "https://esm.sh/v135/@radix-ui/",
    "@radix-ui/react-dialog": "https://esm.sh/v135/*@radix-ui/react-dialog@1.1.4",
    "@radix-ui/react-separator": "https://esm.sh/v135/*@radix-ui/react-separator@1.1.1",
    "@radix-ui/react-slot": "https://esm.sh/v135/*@radix-ui/react-slot@1.1.1",
    "@radix-ui/react-tooltip": "https://esm.sh/v135/*@radix-ui/react-tooltip@1.1.6",
    "classnames": "https://esm.sh/v135/classnames@2.5.1",
    "classnames/": "https://esm.sh/v135/classnames@2.5.1/",
    "react-dom": "https://esm.sh/v135/react-dom@18.3.1",
    "react-dom/": "https://esm.sh/v135/react-dom@18.3.1/",
    "react": "https://esm.sh/v135/react@18.3.1",
    "react/": "https://esm.sh/v135/react@18.3.1/",
    "valtio": "npm:valtio@2.1.2"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "export TASK=prod && deno task lume",
    "serve": "export TASK=dev && deno task lume -s",
    "cms": "export TASK=dev && deno task lume cms",
    "esm:add": "deno run -A https://esm.sh/v135 add",
    "esm:update": "deno run -A https://esm.sh/v135 update",
    "esm:remove": "deno run -A https://esm.sh/v135 remove"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/v135/react@18.3.1",
    "jsxImportSourceTypes": "https://esm.sh/v135/@types/react@18.3.1",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ]
  },
  "scopes": {
    "https://esm.sh/v135/": {
      "@radix-ui/primitive": "https://esm.sh/v135/@radix-ui/primitive@1.1.1",
      "@radix-ui/react-compose-refs": "https://esm.sh/v135/@radix-ui/react-compose-refs@1.1.1",
      "@radix-ui/react-context": "https://esm.sh/v135/@radix-ui/react-context@1.1.1",
      "@radix-ui/react-dismissable-layer": "https://esm.sh/v135/@radix-ui/react-dismissable-layer@1.1.3",
      "@radix-ui/react-focus-guards": "https://esm.sh/v135/@radix-ui/react-focus-guards@1.1.1",
      "@radix-ui/react-focus-scope": "https://esm.sh/v135/@radix-ui/react-focus-scope@1.1.1",
      "@radix-ui/react-id": "https://esm.sh/v135/@radix-ui/react-id@1.1.0",
      "@radix-ui/react-popper": "https://esm.sh/v135/@radix-ui/react-popper@1.2.1",
      "@radix-ui/react-portal": "https://esm.sh/v135/@radix-ui/react-portal@1.1.3",
      "@radix-ui/react-presence": "https://esm.sh/v135/@radix-ui/react-presence@1.1.2",
      "@radix-ui/react-primitive": "https://esm.sh/v135/@radix-ui/react-primitive@2.0.1",
      "@radix-ui/react-use-controllable-state": "https://esm.sh/v135/@radix-ui/react-use-controllable-state@1.1.0",
      "@radix-ui/react-visually-hidden": "https://esm.sh/v135/@radix-ui/react-visually-hidden@1.1.1",
      "aria-hidden": "https://esm.sh/v135/aria-hidden@1.2.4",
      "react-remove-scroll": "https://esm.sh/v135/react-remove-scroll@2.6.2",
      "scheduler": "https://esm.sh/v135/scheduler@0.25.0"
    }
  }
}
